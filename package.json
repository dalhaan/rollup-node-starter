{
  "name": "rollup-node-cli-starter",
  "version": "0.1.0",
  "description": "A starter boilerplate project for building a Node tool with Rollup.",
  "type": "module",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "files": [
    "lib"
  ],
  "author": "Dallan Freemantle",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node --esm src/index.ts",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.11.16",
    "rollup": "^3.7.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
