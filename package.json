{
  "name": "rollup-node-starter",
  "version": "0.1.0",
  "description": "A starter boilerplate project for building a Node tool with Rollup.",
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/es/index.mjs",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/es/index.mjs",
      "default": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "author": "Dallan Freemantle",
  "license": "MIT",
  "repository": "https://github.com/dalhaan/rollup-node-starter",
  "private": true,
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "build": "rm -rf dist && rollup -c",
    "test": "jest",
    "check:lint": "eslint .",
    "check:types": "tsc --noEmit",
    "check:all": "npm-run-all --parallel check:lint check:types"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.11.16",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
